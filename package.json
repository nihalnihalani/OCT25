{
  "name": "bud-dy",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "build:firebase": "next build",
    "check-env": "node scripts/check-env.js",
    "check-env:fix": "node scripts/check-env.js --fix",
    "test": "node node_modules/jest/bin/jest.js",
    "test:watch": "node node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node node_modules/jest/bin/jest.js --coverage",
    "test:unit": "node node_modules/jest/bin/jest.js tests/unit",
    "test:integration": "node node_modules/jest/bin/jest.js tests/integration",
    "test:verbose": "node node_modules/jest/bin/jest.js --verbose",
    "predev": "node scripts/check-env.js",
    "start": "next start",
    "lint": "next lint",
    "deploy": "npm run build:firebase && firebase deploy",
    "deploy:hosting": "npm run build:firebase && firebase deploy --only hosting",
    "deploy:apphosting": "firebase apphosting:backends:create --location=us-central1",
    "upload-media": "node scripts/upload-media.js"
  },
  "dependencies": {
    "@tailwindcss/postcss": "^4.1.11",
    "autoprefixer": "^10.4.21",
    "firebase": "^10.0.0",
    "firebaseui": "^6.1.0",
    "nanoid": "^5.1.6",
    "next": "^14.2.30",
    "openai": "^5.10.1",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^7.7.0",
    "recharts": "^3.1.0",
    "tailwindcss": "^4.1.11"
  },
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "@babel/preset-react": "^7.22.0",
    "@babel/preset-typescript": "^7.23.0",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.19.9",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "babel-jest": "^29.7.0",
    "dotenv": "^17.2.0",
    "eslint": "^8.57.1",
    "eslint-config-next": "^14.0.4",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.8.3"
  }
}
